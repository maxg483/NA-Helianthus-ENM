  Hel_dec_DF <- Synonym_Scrub(Hel_dec, Helianthus_decapetalus, 727, Hel_dec_FN, Hel_dec_DF)
  Hel_des_DF <- Synonym_Scrub(Hel_des, Helianthus_deserticola, 74, Hel_des_FN, Hel_des_DF)
  Hel_div_DF <- Synonym_Scrub(Hel_div, Helianthus_divaricatus, 1143, Hel_div_FN, Hel_div_DF)
